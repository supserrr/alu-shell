#!/usr/bin/env bash
# Finds processes containing "bash" in their name

# Iterate over all running processes
for pid in /proc/*/cmdline; do
  # Read the command line arguments from the file
  command=$(cat "$pid")
  # Check if "bash" is in the command
  if [[ "$command" =~ bash ]]; then
    # Extract the PID from the filename
    process_pid=$(basename "$pid")
    # Print the PID and command
    echo "$process_pid $command"
  fi
done
